# get results from a unix command
  my $command="UNIX_COMMAND |";
# put results to   a unix command
# $command="| UNIX_COMMAND";

  open(COMMAND,"$command") || die("$name: Cannot COMMAND_DISCRIPTION with '$command': $!.");
  my(@results)=<COMMAND>;
  close(COMMAND);

