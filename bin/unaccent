#!/usr/bin/env perl
#
# A perl script to change unicode utf-8 Latin-1 Supplement character
# code sequences # 303|4|5 2xx
# to their closest ascii equivalents, which for most, removes the accent.
# keywords: utf8 utf-8 ascii windows
#
$name = $0; $name =~ s'.*/''; # remove path--like basename
$usage = "usage:\n$name [-opt1] [-opt2] [-opt3]";

while (<>) {

if (/[\303\304\305]/) {
  s/\303\200/A/g;   # LATIN CAPITAL LETTER A WITH GRAVE
  s/\303\201/A/g;   # LATIN CAPITAL LETTER A WITH ACUTE
  s/\303\202/A/g;   # LATIN CAPITAL LETTER A WITH CIRCUMFLEX
  s/\303\203/A/g;   # LATIN CAPITAL LETTER A WITH TILDE
  s/\303\204/A/g;   # LATIN CAPITAL LETTER A WITH DIAERESIS
  s/\303\205/A/g;   # LATIN CAPITAL LETTER A WITH RING ABOVE
  s/\303\206/AE/g;  # LATIN CAPITAL LETTER AE
  s/\303\207/C/g;   # LATIN CAPITAL LETTER C WITH CEDILLA
  s/\303\210/E/g;   # LATIN CAPITAL LETTER E WITH GRAVE
  s/\303\211/E/g;   # LATIN CAPITAL LETTER E WITH ACUTE
  s/\303\212/E/g;   # LATIN CAPITAL LETTER E WITH CIRCUMFLEX
  s/\303\213/E/g;   # LATIN CAPITAL LETTER E WITH DIAERESIS
  s/\303\214/I/g;   # LATIN CAPITAL LETTER I WITH GRAVE
  s/\303\215/I/g;   # LATIN CAPITAL LETTER I WITH ACUTE
  s/\303\216/I/g;   # LATIN CAPITAL LETTER I WITH CIRCUMFLEX
  s/\303\217/I/g;   # LATIN CAPITAL LETTER I WITH DIAERESIS
  s/\303\220/D/g;   # LATIN CAPITAL LETTER ETH
  s/\303\221/N/g;   # LATIN CAPITAL LETTER N WITH TILDE
  s/\303\222/O/g;   # LATIN CAPITAL LETTER O WITH GRAVE
  s/\303\223/O/g;   # LATIN CAPITAL LETTER O WITH ACUTE
  s/\303\224/O/g;   # LATIN CAPITAL LETTER O WITH CIRCUMFLEX
  s/\303\225/O/g;   # LATIN CAPITAL LETTER O WITH TILDE
  s/\303\226/O/g;   # LATIN CAPITAL LETTER O WITH DIAERESIS
  s/\303\227/X/g;   # MULTIPLICATION SIGN
  s/\303\230/O/g;   # LATIN CAPITAL LETTER O WITH STROKE
  s/\303\231/U/g;   # LATIN CAPITAL LETTER U WITH GRAVE
  s/\303\232/U/g;   # LATIN CAPITAL LETTER U WITH ACUTE
  s/\303\233/U/g;   # LATIN CAPITAL LETTER U WITH CIRCUMFLEX
  s/\303\234/U/g;   # LATIN CAPITAL LETTER U WITH DIAERESIS
  s/\303\235/Y/g;   # LATIN CAPITAL LETTER Y WITH ACUTE
  s/\303\236/p/g;   # LATIN CAPITAL LETTER THORN
  s/\303\237/ss/g;  # LATIN SMALL LETTER SHARP S
  s/\303\240/a/g;   # LATIN SMALL LETTER A WITH GRAVE
  s/\303\241/a/g;   # LATIN SMALL LETTER A WITH ACUTE
  s/\303\242/a/g;   # LATIN SMALL LETTER A WITH CIRCUMFLEX
  s/\303\243/a/g;   # LATIN SMALL LETTER A WITH TILDE
  s/\303\244/a/g;   # LATIN SMALL LETTER A WITH DIAERESIS
  s/\303\245/a/g;   # LATIN SMALL LETTER A WITH RING ABOVE
  s/\303\246/ae/g;  # LATIN SMALL LETTER AE
  s/\303\247/c/g;   # LATIN SMALL LETTER C WITH CEDILLA
  s/\303\250/e/g;   # LATIN SMALL LETTER E WITH GRAVE
  s/\303\251/e/g;   # LATIN SMALL LETTER E WITH ACUTE
  s/\303\252/e/g;   # LATIN SMALL LETTER E WITH CIRCUMFLEX
  s/\303\253/e/g;   # LATIN SMALL LETTER E WITH DIAERESIS
  s/\303\254/i/g;   # LATIN SMALL LETTER I WITH GRAVE
  s/\303\255/i/g;   # LATIN SMALL LETTER I WITH ACUTE
  s/\303\256/i/g;   # LATIN SMALL LETTER I WITH CIRCUMFLEX
  s/\303\257/i/g;   # LATIN SMALL LETTER I WITH DIAERESIS
  s/\303\260/d/g;   # LATIN SMALL LETTER ETH
  s/\303\261/n/g;   # LATIN SMALL LETTER N WITH TILDE
  s/\303\262/o/g;   # LATIN SMALL LETTER O WITH GRAVE
  s/\303\263/o/g;   # LATIN SMALL LETTER O WITH ACUTE
  s/\303\264/o/g;   # LATIN SMALL LETTER O WITH CIRCUMFLEX
  s/\303\265/o/g;   # LATIN SMALL LETTER O WITH TILDE
  s/\303\266/o/g;   # LATIN SMALL LETTER O WITH DIAERESIS
  s,\303\267,/,g;   # DIVISION SIGN
  s/\303\270/o/g;   # LATIN SMALL LETTER O WITH STROKE
  s/\303\271/u/g;   # LATIN SMALL LETTER U WITH GRAVE
  s/\303\272/u/g;   # LATIN SMALL LETTER U WITH ACUTE
  s/\303\273/u/g;   # LATIN SMALL LETTER U WITH CIRCUMFLEX
  s/\303\274/u/g;   # LATIN SMALL LETTER U WITH DIAERESIS
  s/\303\275/y/g;   # LATIN SMALL LETTER Y WITH ACUTE
  s/\303\276/p/g;   # LATIN SMALL LETTER THORN
  s/\303\277/y/g;   # LATIN SMALL LETTER Y WITH DIAERESIS
  s/\304\200/A/g;   # LATIN CAPITAL LETTER A WITH MACRON
  s/\304\201/a/g;   # LATIN SMALL LETTER A WITH MACRON
  s/\304\202/A/g;   # LATIN CAPITAL LETTER A WITH BREVE
  s/\304\203/a/g;   # LATIN SMALL LETTER A WITH BREVE
  s/\304\204/A/g;   # LATIN CAPITAL LETTER A WITH OGONEK
  s/\304\205/a/g;   # LATIN SMALL LETTER A WITH OGONEK
  s/\304\206/C/g;   # LATIN CAPITAL LETTER C WITH ACUTE
  s/\304\207/c/g;   # LATIN SMALL LETTER C WITH ACUTE
  s/\304\210/C/g;   # LATIN CAPITAL LETTER C WITH CIRCUMFLEX
  s/\304\211/c/g;   # LATIN SMALL LETTER C WITH CIRCUMFLEX
  s/\304\212/C/g;   # LATIN CAPITAL LETTER C WITH DOT ABOVE
  s/\304\213/c/g;   # LATIN SMALL LETTER C WITH DOT ABOVE
  s/\304\214/C/g;   # LATIN CAPITAL LETTER C WITH CARON
  s/\304\215/c/g;   # LATIN SMALL LETTER C WITH CARON
  s/\304\216/D/g;   # LATIN CAPITAL LETTER D WITH CARON
  s/\304\217/d/g;   # LATIN SMALL LETTER D WITH CARON
  s/\304\220/D/g;   # LATIN CAPITAL LETTER D WITH STROKE
  s/\304\221/d/g;   # LATIN SMALL LETTER D WITH STROKE
  s/\304\222/E/g;   # LATIN CAPITAL LETTER E WITH MACRON
  s/\304\223/e/g;   # LATIN SMALL LETTER E WITH MACRON
  s/\304\224/E/g;   # LATIN CAPITAL LETTER E WITH BREVE
  s/\304\225/e/g;   # LATIN SMALL LETTER E WITH BREVE
  s/\304\226/E/g;   # LATIN CAPITAL LETTER E WITH DOT ABOVE
  s/\304\227/e/g;   # LATIN SMALL LETTER E WITH DOT ABOVE
  s/\304\230/E/g;   # LATIN CAPITAL LETTER E WITH OGONEK
  s/\304\231/e/g;   # LATIN SMALL LETTER E WITH OGONEK
  s/\304\232/E/g;   # LATIN CAPITAL LETTER E WITH CARON
  s/\304\233/e/g;   # LATIN SMALL LETTER E WITH CARON
  s/\304\234/G/g;   # LATIN CAPITAL LETTER G WITH CIRCUMFLEX
  s/\304\235/g/g;   # LATIN SMALL LETTER G WITH CIRCUMFLEX
  s/\304\236/G/g;   # LATIN CAPITAL LETTER G WITH BREVE
  s/\304\237/g/g;   # LATIN SMALL LETTER G WITH BREVE
  s/\304\240/G/g;   # LATIN CAPITAL LETTER G WITH DOT ABOVE
  s/\304\241/g/g;   # LATIN SMALL LETTER G WITH DOT ABOVE
  s/\304\242/G/g;   # LATIN CAPITAL LETTER G WITH CEDILLA
  s/\304\243/g/g;   # LATIN SMALL LETTER G WITH CEDILLA
  s/\304\244/H/g;   # LATIN CAPITAL LETTER H WITH CIRCUMFLEX
  s/\304\245/h/g;   # LATIN SMALL LETTER H WITH CIRCUMFLEX
  s/\304\246/H/g;   # LATIN CAPITAL LETTER H WITH STROKE
  s/\304\247/h/g;   # LATIN SMALL LETTER H WITH STROKE
  s/\304\250/I/g;   # LATIN CAPITAL LETTER I WITH TILDE
  s/\304\251/i/g;   # LATIN SMALL LETTER I WITH TILDE
  s/\304\252/I/g;   # LATIN CAPITAL LETTER I WITH MACRON
  s/\304\253/i/g;   # LATIN SMALL LETTER I WITH MACRON
  s/\304\254/I/g;   # LATIN CAPITAL LETTER I WITH BREVE
  s/\304\255/i/g;   # LATIN SMALL LETTER I WITH BREVE
  s/\304\256/I/g;   # LATIN CAPITAL LETTER I WITH OGONEK
  s/\304\257/i/g;   # LATIN SMALL LETTER I WITH OGONEK
  s/\304\260/I/g;   # LATIN CAPITAL LETTER I WITH DOT ABOVE
  s/\304\261/i/g;   # LATIN SMALL LETTER DOTLESS I
  s/\304\262/IJ/g;  # LATIN CAPITAL LIGATURE IJ
  s/\304\263/ij/g;  # LATIN SMALL LIGATURE IJ
  s/\304\264/J/g;   # LATIN CAPITAL LETTER J WITH CIRCUMFLEX
  s/\304\265/j/g;   # LATIN SMALL LETTER J WITH CIRCUMFLEX
  s/\304\266/K/g;   # LATIN CAPITAL LETTER K WITH CEDILLA
  s/\304\267/k/g;   # LATIN SMALL LETTER K WITH CEDILLA
  s/\304\270/k/g;   # LATIN SMALL LETTER KRA
  s/\304\271/L/g;   # LATIN CAPITAL LETTER L WITH ACUTE
  s/\304\272/l/g;   # LATIN SMALL LETTER L WITH ACUTE
  s/\304\273/L/g;   # LATIN CAPITAL LETTER L WITH CEDILLA
  s/\304\274/l/g;   # LATIN SMALL LETTER L WITH CEDILLA
  s/\304\275/L/g;   # LATIN CAPITAL LETTER L WITH CARON
  s/\304\276/l/g;   # LATIN SMALL LETTER L WITH CARON
  s/\304\277/L/g;   # LATIN CAPITAL LETTER L WITH MIDDLE DOT
  s/\305\200/l/g;   # LATIN SMALL LETTER L WITH MIDDLE DOT
  s/\305\201/L/g;   # LATIN CAPITAL LETTER L WITH STROKE
  s/\305\202/l/g;   # LATIN SMALL LETTER L WITH STROKE
  s/\305\203/N/g;   # LATIN CAPITAL LETTER N WITH ACUTE
  s/\305\204/n/g;   # LATIN SMALL LETTER N WITH ACUTE
  s/\305\205/N/g;   # LATIN CAPITAL LETTER N WITH CEDILLA
  s/\305\206/n/g;   # LATIN SMALL LETTER N WITH CEDILLA
  s/\305\207/N/g;   # LATIN CAPITAL LETTER N WITH CARON
  s/\305\210/n/g;   # LATIN SMALL LETTER N WITH CARON
  s/\305\211/n/g;   # LATIN SMALL LETTER N PRECEDED BY APOSTROPHE
  s/\305\212/N/g;   # LATIN CAPITAL LETTER ENG
  s/\305\213/n/g;   # LATIN SMALL LETTER ENG
  s/\305\214/O/g;   # LATIN CAPITAL LETTER O WITH MACRON
  s/\305\215/o/g;   # LATIN SMALL LETTER O WITH MACRON
  s/\305\216/O/g;   # LATIN CAPITAL LETTER O WITH BREVE
  s/\305\217/o/g;   # LATIN SMALL LETTER O WITH BREVE
  s/\305\220/O/g;   # LATIN CAPITAL LETTER O WITH DOUBLE ACUTE
  s/\305\221/o/g;   # LATIN SMALL LETTER O WITH DOUBLE ACUTE
  s/\305\222/OE/g;  # LATIN CAPITAL LIGATURE OE
  s/\305\223/oe/g;  # LATIN SMALL LIGATURE OE
  s/\305\224/R/g;   # LATIN CAPITAL LETTER R WITH ACUTE
  s/\305\225/r/g;   # LATIN SMALL LETTER R WITH ACUTE
  s/\305\226/R/g;   # LATIN CAPITAL LETTER R WITH CEDILLA
  s/\305\227/r/g;   # LATIN SMALL LETTER R WITH CEDILLA
  s/\305\230/R/g;   # LATIN CAPITAL LETTER R WITH CARON
  s/\305\231/r/g;   # LATIN SMALL LETTER R WITH CARON
  s/\305\232/S/g;   # LATIN CAPITAL LETTER S WITH ACUTE
  s/\305\233/s/g;   # LATIN SMALL LETTER S WITH ACUTE
  s/\305\234/S/g;   # LATIN CAPITAL LETTER S WITH CIRCUMFLEX
  s/\305\235/s/g;   # LATIN SMALL LETTER S WITH CIRCUMFLEX
  s/\305\236/S/g;   # LATIN CAPITAL LETTER S WITH CEDILLA
  s/\305\237/s/g;   # LATIN SMALL LETTER S WITH CEDILLA
  s/\305\240/S/g;   # LATIN CAPITAL LETTER S WITH CARON
  s/\305\241/s/g;   # LATIN SMALL LETTER S WITH CARON
  s/\305\242/T/g;   # LATIN CAPITAL LETTER T WITH CEDILLA
  s/\305\243/t/g;   # LATIN SMALL LETTER T WITH CEDILLA
  s/\305\244/T/g;   # LATIN CAPITAL LETTER T WITH CARON
  s/\305\245/t/g;   # LATIN SMALL LETTER T WITH CARON
  s/\305\246/T/g;   # LATIN CAPITAL LETTER T WITH STROKE
  s/\305\247/t/g;   # LATIN SMALL LETTER T WITH STROKE
  s/\305\250/U/g;   # LATIN CAPITAL LETTER U WITH TILDE
  s/\305\251/u/g;   # LATIN SMALL LETTER U WITH TILDE
  s/\305\252/U/g;   # LATIN CAPITAL LETTER U WITH MACRON
  s/\305\253/u/g;   # LATIN SMALL LETTER U WITH MACRON
  s/\305\254/U/g;   # LATIN CAPITAL LETTER U WITH BREVE
  s/\305\255/u/g;   # LATIN SMALL LETTER U WITH BREVE
  s/\305\256/U/g;   # LATIN CAPITAL LETTER U WITH RING ABOVE
  s/\305\257/u/g;   # LATIN SMALL LETTER U WITH RING ABOVE
  s/\305\260/U/g;   # LATIN CAPITAL LETTER U WITH DOUBLE ACUTE
  s/\305\261/u/g;   # LATIN SMALL LETTER U WITH DOUBLE ACUTE
  s/\305\262/U/g;   # LATIN CAPITAL LETTER U WITH OGONEK
  s/\305\263/u/g;   # LATIN SMALL LETTER U WITH OGONEK
  s/\305\264/W/g;   # LATIN CAPITAL LETTER W WITH CIRCUMFLEX
  s/\305\265/w/g;   # LATIN SMALL LETTER W WITH CIRCUMFLEX
  s/\305\266/Y/g;   # LATIN CAPITAL LETTER Y WITH CIRCUMFLEX
  s/\305\267/y/g;   # LATIN SMALL LETTER Y WITH CIRCUMFLEX
  s/\305\270/Y/g;   # LATIN CAPITAL LETTER Y WITH DIAERESIS
  s/\305\271/Z/g;   # LATIN CAPITAL LETTER Z WITH ACUTE
  s/\305\272/z/g;   # LATIN SMALL LETTER Z WITH ACUTE
  s/\305\273/Z/g;   # LATIN CAPITAL LETTER Z WITH DOT ABOVE
  s/\305\274/z/g;   # LATIN SMALL LETTER Z WITH DOT ABOVE
  s/\305\275/Z/g;   # LATIN CAPITAL LETTER Z WITH CARON
  s/\305\276/z/g;   # LATIN SMALL LETTER Z WITH CARON
  s/\305\277/ss/g;  # LATIN SMALL LETTER LONG S
}

  print;
}

exit 0;

__END__
print "\305\253";


